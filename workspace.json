{
  "version": 2,
  "projects": {
    "adapter": {
      "root": "libs/adapter",
      "sourceRoot": "libs/adapter",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/adapter/**/*.ts"]
          },
          "outputs": ["{options.outputFile}"]
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/adapter"],
          "options": {
            "jestConfig": "libs/adapter/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/libs/adapter",
            "tsConfig": "libs/adapter/tsconfig.lib.json",
            "packageJson": "libs/adapter/package.json",
            "main": "libs/adapter/index.ts",
            "assets": ["libs/adapter/*.md"]
          }
        }
      },
      "tags": []
    },
