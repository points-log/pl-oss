{
  "version": 2,
  "projects": {
    "adapters": {
      "root": "libs/adapters",
      "sourceRoot": "libs/adapters/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/adapters/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/adapters"
          ],
          "options": {
            "jestConfig": "libs/adapters/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "domain-core": {
      "root": "libs/domain-core",
      "sourceRoot": "libs/domain-core/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/domain-core/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/domain-core"
          ],
          "options": {
            "jestConfig": "libs/domain-core/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}
